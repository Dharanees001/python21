def extract_last_two_digits(n):
    
    n_str = str(n)
    
    
    if len(n_str) <= 2:
        print("The number of digits should be greater than 2.")
        return None
    
    last_two_digits = int(n_str[-2:])
    return last_two_digits

def main():
    try:
        n = int(input("Enter an integer greater than 2: "))
        
        last_two_digits = extract_last_two_digits(n)
        if last_two_digits is not None:
            print("The last two digits of", n, "are:", last_two_digits)
    except ValueError:
        print("Please enter a valid integer.")

if __name__ == "__main__":
    main()
