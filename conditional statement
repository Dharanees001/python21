def can_form_rectangle(a, b, c, d):
    sides = sorted([a, b, c, d])
    
    if sides[0] == sides[1] and sides[2] == sides[3]:
        return True
    else:
        return False

def main():
    try:
        sides = input("Enter the four sides of the rectangle separated by space: ").split()
        a, b, c, d = map(int, sides)
        
        if can_form_rectangle(a, b, c, d):
            print("Yes")
        else:
            print("No")
    except ValueError:
        print("Please enter four valid integers.")

if __name__ == "__main__":
    main()
